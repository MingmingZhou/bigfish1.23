<?xml version="1.0" encoding="UTF-8"?>

<!--
    To add HTML to a message escape all angle brackets < > in tags with  &lt; = < &gt = >
    Then where you use the message in ftl use the following ${StringUtil.wrapString([message])}
-->

<resource xmlns:xsi="http://www.w3.org/01/XMLSchema-instance">

    <property key="ALERT_JOB_SANDBOX_ROWS">
        <value xml:lang="en">100000</value>
        <description>If the total number of rows in the JOB_SANDBOX entity exceeds the parameter value then trigger an alert.</description>
    </property>
</resource>